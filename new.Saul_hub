-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –∫—Ä–∞—Å–∏–≤–æ–π –∫–Ω–æ–ø–∫–∏ (–†–ê–ó–ú–ï–†–´ –ö–ù–û–ü–û–ö –ù–ï –ò–ó–ú–ï–ù–ï–ù–´)
local function createButton(parent, text, position, color, icon)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 220, 0, 45)  -- –†–∞–∑–º–µ—Ä –∫–Ω–æ–ø–æ–∫ —Å–æ—Ö—Ä–∞–Ω–µ–Ω
    button.Position = position
    button.BackgroundColor3 = color
    button.Text = icon and (icon .. " " .. text) or text
    button.TextColor3 = Colors.Text
    button.Font = Enum.Font.GothamSemibold
    button.TextSize = 14
    button.AutoButtonColor = false
    button.Parent = parent

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = button

    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, color),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(
            math.floor(color.R * 200),
            math.floor(color.G * 200), 
            math.floor(color.B * 200)
        ))
    })
    gradient.Rotation = -5
    gradient.Parent = button

    -- –ê–Ω–∏–º–∞—Ü–∏—è –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏ (–†–ê–ó–ú–ï–†–´ –ö–ù–û–ü–û–ö –ù–ï –ò–ó–ú–ï–ù–ï–ù–´)
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundTransparency = 0.1,
            Size = UDim2.new(0, 225, 0, 47)
        }):Play()
    end)

    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundTransparency = 0,
            Size = UDim2.new(0, 220, 0, 45)
        }):Play()
    end)

    -- –≠—Ñ—Ñ–µ–∫—Ç –Ω–∞–∂–∞—Ç–∏—è (–†–ê–ó–ú–ï–†–´ –ö–ù–û–ü–û–ö –ù–ï –ò–ó–ú–ï–ù–ï–ù–´)
    button.MouseButton1Down:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.1), {
            BackgroundTransparency = 0.3,
            Size = UDim2.new(0, 215, 0, 43)
        }):Play()
    end)

    button.MouseButton1Up:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.1), {
            BackgroundTransparency = 0,
            Size = UDim2.new(0, 220, 0, 45)
        }):Play()
    end)

    return button
end

