            currentGameLabel.Position = UDim2.new(0, 10, 0, yPosition)
            currentGameLabel.BackgroundTransparency = 1
            currentGameLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
            currentGameLabel.Font = Enum.Font.GothamBold
            currentGameLabel.TextSize = 16
            currentGameLabel.TextXAlignment = Enum.TextXAlignment.Left
            currentGameLabel.Parent = tabContent
            
            yPosition = yPosition + 35

            -- –ö–Ω–æ–ø–∫–∏ –¥–ª—è —Ç–µ–∫—É—â–µ–π –∏–≥—Ä—ã
            for j, scriptData in ipairs(currentGameData.scripts) do
