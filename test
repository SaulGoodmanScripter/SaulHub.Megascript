    contentTitle.Parent = tabContent

    -- –°–æ–¥–µ—Ä–∂–∏–º–æ–µ –≤–∫–ª–∞–¥–æ–∫
    elseif tabName == "Games" then
    local currentGameId = game.PlaceId
    local currentGameData = gameDatabase[currentGameId]
    local yPosition = 50

    -- –°–æ–∑–¥–∞–µ–º ScrollingFrame –¥–ª—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∏
    local scrollFrame = Instance.new("ScrollingFrame")
