                btnCorner.CornerRadius = UDim.new(0, 6)
                btnCorner.Parent = button

                button.MouseButton1Click:Connect(function()
                    print("–ó–∞–≥—Ä—É–∂–∞–µ–º: " .. scriptData[1])
                    loadstring(game:HttpGet(scriptData[2]))()
