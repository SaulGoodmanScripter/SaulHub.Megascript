        yPosition = yPosition + 35

        -- –ö–Ω–æ–ø–∫–∏ –¥–ª—è —Ç–µ–∫—É—â–µ–π –∏–≥—Ä—ã
        for j, scriptData in ipairs(currentGameData.scripts) do
            local button = Instance.new("TextButton")
            button.Size = UDim2.new(0, 250, 0, 40)
            button.Position = UDim2.new(0, 20, 0, yPosition)
            button.BackgroundColor3 = scriptData[3] or Color3.fromRGB(100, 200, 100)
            button.Text = scriptData[1]
            button.TextColor3 = Color3.fromRGB(255, 255, 255)
            button.Font = Enum.Font.Gotham
            button.TextSize = 13
            button.Parent = scrollFrame

