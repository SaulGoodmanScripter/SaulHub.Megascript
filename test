        local currentGameId = game.PlaceId
        local currentGameData = gameDatabase[currentGameId]
        local yPosition = 50

        -- –ï—Å–ª–∏ –∏–≥—Ä–∞ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç—Å—è
        if currentGameData then
            local currentGameLabel = Instance.new("TextLabel")
            currentGameLabel.Text = "üéØ –°–ö–†–ò–ü–¢–´ –î–õ–Ø –¢–ï–ö–£–©–ï–ô –ò–ì–†–´:"
