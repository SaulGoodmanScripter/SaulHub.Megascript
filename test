        }

        for j, btnData in ipairs(universalButtons) do
            local button = Instance.new("TextButton")
            button.Size = UDim2.new(0, 200, 0, 40)
            button.Position = UDim2.new(0, 20, 0, 50 + (j-1) * 50)
