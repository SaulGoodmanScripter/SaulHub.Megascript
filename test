    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, yPosition + 10)
end

    elseif tabName == "Universal" then
        local universalButtons = {
            {"Esp Player", "https://raw.githubusercontent.com/SaulGoodmanScripter/SaulHub.Megascript/refs/heads/main/especially%20player%20"},
