                Title = "Script Hub - –û—à–∏–±–∫–∞",
                Text = "–û—à–∏–±–∫–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è: " .. tostring(loadResult),
                Duration = 5,
